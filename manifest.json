{
    "name" : "bookmarks2markdown",
    "version" : "0.0.1",
    "manifest_version" : 2,
    "description" : "ブックマークのデータをマークダウン記法に変換する拡張.",
    "permissions" : [
        "bookmarks"
    ],
    "browser_action": {
        "default_title": "My Bookmarks",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
